{"routes":[
    {
        "path": "/api/v1/main/*path",
        "targetURL": "http://receipt-service:8000",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "stripPrefix":"",
        "requireAuth": true,
        "cacheable":false,
        "timeout":20
    },
    {
        "path":"/api/v1/auth/*path",
        "targetURL":"http://auth-service:8000/api/v1/auth",
        "methods":["GET","POST","PUT","DELETE"],
        "stripPrefix":"",
        "requireAuth":false,
        "cacheable":false,
        "timeout":20
    },
    {
        "path":"/api/v1/users/*path",
        "targetURL":"http://auth-service:8000/api/v1/users",
        "methods":["GET","POST","PUT","DELETE"],
        "stripPrefix":"",
        "requireAuth":true,
        "cacheable":false,
        "timeout":20
    },
    {
        "path":"/*path",
        "targetURL":"http://web-client:3000",
        "methods":["GET","POST","PUT","DELETE"],
        "stripPrefix":"",
        "requireAuth":false,
        "cacheable":false,
        "timeout":20
    }
]}